generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model Agendamento {
  id           Int      @id @default(autoincrement())
  cliente      String
  data         DateTime
  servico      String
  criadoEm     DateTime @default(now())
  atualizadoEm DateTime @updatedAt
}
